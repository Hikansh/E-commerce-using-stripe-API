<h1>Profile</h1>
<hr>
<h3>My Orders</h3>
<br>
{{# each orders }}
<ul class="list-group">
    <li class="list-group-item"><b>Order ID :</b> {{this.id}}</li>
    <li class="list-group-item"><b>Product ID :</b> {{this.product}}</li>
    <li class="list-group-item"><b>Payment ID :</b> {{this.paymentId}}</li>
    <li class="list-group-item"><b>Estimated delivery :</b> {{this.delivery}} Days</li>
</ul>
<hr>
{{/each}}